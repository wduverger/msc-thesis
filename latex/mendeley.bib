doi = {10.1016/j.colsurfb.2018.06.035},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Ismael et al. - 2018 - Unraveling the molecular mechanism of the effects of sodium dodecyl sulfate, salts, and sugars on amyloid fibril.pdf:pdf},
issn = {18734367},
journal = {Colloids and Surfaces B: Biointerfaces},
keywords = {Amyloid fibril,Anionic surfactant,Camel IgG,Protein aggregation,SDS,pH},
number = {June},
pages = {430--437},
pmid = {29957532},
publisher = {Elsevier},
title = {{Unraveling the molecular mechanism of the effects of sodium dodecyl sulfate, salts, and sugars on amyloid fibril formation in camel IgG}},
