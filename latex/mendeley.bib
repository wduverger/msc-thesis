file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Chouraki et al. - 2015 - Plasma amyloid-$\beta$ and risk of Alzheimer's disease in the Framingham Heart Study.pdf:pdf},
issn = {15525279},
journal = {Alzheimer's and Dementia},
keywords = {Alzheimer's disease,A$\beta$ peptides,Epidemiology,Framingham,Heart,Incident,Incident dementia,Meta-analysis,Plasma biomarker,Study},
number = {3},
pages = {249--257.e1},
pmid = {25217292},
publisher = {Elsevier Ltd},
title = {{Plasma amyloid-$\beta$ and risk of Alzheimer's disease in the Framingham Heart Study}},
