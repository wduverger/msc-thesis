}
@article{Chouraki2015,
abstract = {Background Plasma amyloid-$\beta$ (A$\beta$) peptide levels have been examined as a low-cost accessible marker for risk of incident Alzheimer's disease (AD) and dementia, but results have varied between studies. We reassessed these associations in one of the largest, prospective, community-based studies to date. Methods A total of 2189 dementia-free, Framingham Study participants aged {\textgreater}60 years (mean age, 72 ± 8 years; 56{\%} women) had plasma A$\beta$1-42 and A$\beta$1-40 measured and were followed prospectively (mean, 7.6 ± 3.0 years) for dementia/AD. Results Increased plasma A$\beta$1-42 levels were associated with lower risk of dementia (A$\beta$1-42: hazard ratio [HR] = 0.80 [0.71{\^{a}}€ 0.90], P {\textless}.001; A$\beta$1-42-to-A$\beta$1-40 ratio: HR = 0.86 [0.76{\^{a}}€ 0.98], P =.027) and AD (A$\beta$1-42: HR = 0.79 [0.69{\^{a}}€ 0.90], P {\textless}.001; A$\beta$1-42-to-A$\beta$1-40 ratio: HR = 0.83 [0.72{\^{a}}€ 0.96], P =.012). Conclusion Our results suggest that lower plasma A$\beta$ levels are associated with risk of incident AD and dementia. They encourage further evaluation of plasma A$\beta$ levels as a biomarker for risk of developing clinical AD and dementia.},
author = {Chouraki, Vincent and Beiser, Alexa and Younkin, Linda and Preis, Sarah Rosner and Weinstein, Galit and Hansson, Oskar and Skoog, Ingmar and Lambert, Jean Charles and Au, Rhoda and Launer, Lenore and Wolf, Philip A. and Younkin, Steven and Seshadri, Sudha},
