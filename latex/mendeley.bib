publisher = {Springer US},
title = {{Plasmonic nanoparticle amyloid corona for screening A$\beta$ oligomeric aggregate-degrading drugs}},
url = {https://doi.org/10.1038/s41467-020-20611-4{\%}0Ahttp://www.nature.com/articles/s41467-020-20611-4},
