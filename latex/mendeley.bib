file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Foley et al. - 2020 - Evidence for aggregation-independent, PrPC-mediated A$\beta$ cellular internalization.pdf:pdf},
isbn = {2009238117},
issn = {10916490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Alzheimer's disease | amyloid $\beta$ | prion protein (P},
