journal = {International Journal of Molecular Sciences},
keywords = {Liquid–liquid phase separation,Low-complexity domains,Prion,Prion-like domain,Stress granules},
number = {3},
pages = {1--19},
title = {{From prions to stress granules: Defining the compositional features of prion-like domains that promote different types of assemblies}},
