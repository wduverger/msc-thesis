file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Xue et al. - 2009 - Fibril fragmentation enhances amyloid cytotoxicity.pdf:pdf},
issn = {00219258},
journal = {Journal of Biological Chemistry},
number = {49},
pages = {34272--34282},
pmid = {19808677},
publisher = {{\^{A}}{\textcopyright} 2009 ASBMB. Currently published by Elsevier Inc; originally published by American Society for Biochemistry and Molecular Biology.},
title = {{Fibril fragmentation enhances amyloid cytotoxicity}},
url = {http://dx.doi.org/10.1074/jbc.M109.049809},
volume = {284},
year = {2009}
}
