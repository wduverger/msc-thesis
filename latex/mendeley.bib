doi = {10.1002/jmri.23642},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Plewes, Kucharczyk - 2012 - Physics of MRI A primer.pdf:pdf;:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Plewes, Kucharczyk - 2012 - Physics of MRI A primer(2).pdf:pdf},
issn = {10531807},
