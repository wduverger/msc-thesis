issn = {14765403},
journal = {Cell Death and Differentiation},
number = {10},
pages = {1784--1798},
pmid = {28644435},
publisher = {Nature Publishing Group},
title = {{P53 amyloid formation leading to its loss of function: Implications in cancer pathogenesis}},
url = {http://dx.doi.org/10.1038/cdd.2017.105},
volume = {24},
year = {2017}
}
@article{Pieta2021,
abstract = {Alzheimer's disease (AD) is characterized by progressive neurodegeneration associated with amyloid $\beta$ (A$\beta$) peptide aggregation. The aggregation of A$\beta$ monomers (A$\beta$Ms) leads to the formation of A$\beta$ oligomers (A$\beta$Os), the neurotoxic A$\beta$ form, capable of permeating the cell membrane. Here, we investigated the effect of a fluorene-based active drug candidate, named K162, on both A$\beta$ aggregation and A$\beta$O toxicity toward the bilayer lipid membrane (BLM). Electrochemical impedance spectroscopy (EIS), atomic force microscopy (AFM), and molecular dynamics (MD) were employed to show that K162 inhibits A$\beta$Os-induced BLM permeation, thus preserving BLM integrity. In the presence of K162, only shallow defects on the BLM surface were formed. Apparently, K162 modifies A$\beta$ aggregation by bypassing the formation of toxic A$\beta$Os, and only nontoxic A$\beta$Ms, dimers (A$\beta$Ds), and fibrils (A$\beta$Fs) are produced. Unlike other A$\beta$ toxicity inhibitors, K162 preserves neurologically beneficial A$\beta$Ms. This unique K162 inhibition mechanism provides an alternative AD therapeutic strategy that could be explored in the future.},
author = {Pieta, Piotr and Mrdenovic, Dusan and Zarzycki, Piotr and Majewska, Marta and Pieta, Izabela S. and Nowakowski, Robert and Kutner, Wlodzimierz and Lipkowski, Jacek},
doi = {10.1021/acschemneuro.0c00754},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Pieta et al. - 2021 - Inhibition of amyloid $\beta$-induced lipid membrane permeation and amyloid $\beta$ aggregation by K162.pdf:pdf},
issn = {19487193},
journal = {ACS Chemical Neuroscience},
keywords = {Alzheimer's disease,Amyloid $\beta$,Amyloid $\beta$ aggregation,Atomic force microscopy,Membrane permeation,Toxicity inhibition},
pmid = {33478212},
title = {{Inhibition of amyloid $\beta$-induced lipid membrane permeation and amyloid $\beta$ aggregation by K162}},
volume = {162},
year = {2021}
}
@article{Han2021,
abstract = {Despite the urgent need to image living specimens for cutting-edge biological research, most existing fluorescent labeling methods suffer from either poor optical properties or complicated operations required to realize cell-permeability and specificity. In this study, we introduce a method to overcome these limits—taking advantage of the intrinsic affinity of bright and photostable fluorophores, no matter if they are supposed to be live-cell incompatible or not. Incubated with living cells and tissues in particular conditions (concentration and temperature), some Atto and BODIPY dyes show live-cell labeling capability for specific organelles without physical cell-penetration or chemical modifications. Notably, by using Atto 647N as a live-cell mitochondrial marker, we obtain 2.5-time enhancement of brightness and photostability compared with the most commonly used SiR dye in long-term imaging. Our strategy has expanded the scientist's toolbox for understanding the dynamics and interactions of subcellular structures in living specimens.},
author = {Han, Yubing and Zhang, Zhimin and Liu, Wenjie and Yao, Yuanfa and Xu, Yingke and Liu, Xu and Kuang, Cuifang and Hao, Xiang},
doi = {10.3389/fchem.2020.601436},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Han et al. - 2021 - A Labeling Strategy for Living Specimens in Long-TermSuper-Resolution Fluorescence Imaging.pdf:pdf},
journal = {Frontiers in Chemistry},
keywords = {living specimens,long-term imaging,organic fluorescent dye,subcellular structures,super-resolution microscopy,super-resolution microscopy, organic fluorescent d},
number = {January},
pages = {1--11},
title = {{A Labeling Strategy for Living Specimens in Long-Term/Super-Resolution Fluorescence Imaging}},
volume = {8},
year = {2021}
}
