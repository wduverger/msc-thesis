publisher = {Springer US},
title = {{Infrared nanospectroscopy reveals the molecular interaction fingerprint of an aggregation inhibitor with single A$\beta$42 oligomers}},
url = {http://dx.doi.org/10.1038/s41467-020-20782-0},
volume = {12},
year = {2021}
}
@article{Ries2013,
