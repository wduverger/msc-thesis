journal = {Upsala Journal of Medical Sciences},
keywords = {Antibiotic resistance,Bacteriophage,Phage,Phage therapy},
number = {2},
pages = {192--198},
pmid = {24678769},
title = {{Phage therapy-constraints and possibilities}},
volume = {119},
year = {2014}
}
