doi = {10.1186/1477-3155-8-10},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Breitenstein, H{\"{o}}lzel, Bier - 2010 - Immobilization of different biomolecules by atomic force microscopy.pdf:pdf},
issn = {14773155},
journal = {Journal of Nanobiotechnology},
keywords = {spotting techniques?},
mendeley-tags = {spotting techniques?},
pages = {1--7},
title = {{Immobilization of different biomolecules by atomic force microscopy}},
volume = {8},
year = {2010}
