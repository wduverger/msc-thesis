month = {jul},
number = {1},
pages = {127--136},
title = {{Mapping the Local Organization of Cell Membranes Using Excitation-Polarization-Resolved Confocal Fluorescence Microscopy}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0006349513006309},
volume = {105},
year = {2013}
}
@article{Xue2009,
abstract = {Amyloid fibrils are proteinaceous nano-scale linear aggregates. They are of key interest not only because of their association with numerous disorders, such as type II diabetes mellitus, Alzheimer's and Parkinson's diseases, but also because of their potential to become engineered high-performance nano-materials. Methods to characterise the length distribution of nano-scale linear aggregates such as amyloid fibrils are of paramount importance both in understanding the biological impact of these aggregates and in controlling their mechanical properties as potential nano-materials. Here, we present a new quantitative approach to the determination of the length distribution of amyloid fibrils using tapping-mode atomic force microscopy. The method described employs single-particle image analysis corrected for the length-dependent bias that is a common problem associated with surface-based imaging techniques. Applying this method, we provide a detailed characterisation of the length distribution of samples containing long-straight fibrils formed in vitro from $\beta$2-microglobulin. The results suggest that the Weibull distribution is a suitable model in describing fibril length distributions, and reveal that fibril fragmentation is an important process even under unagitated conditions. These results demonstrate the significance of quantitative length distribution measurements in providing important new information regarding amyloid assembly.},
annote = {PI: SE Radford, Leeds

Summary: quantification of fibril length distribution using AFM in different conditions

Methods
* generate sample with different seed concentrations and stirring conditions
* plating and AFM image analysis
* bias correction: assuming amount of monomers is equal in different samples and adjust measurements to correct for that
* MLE estimation to fit different distributions, and Kolmogorov-Smirnov test for assessing fit quality

Conclusions:
* Weibull (or gamma) distribution seem appropriate
* difference between samples 1 and 2 (diff seeding concentration, no stirring) suggests fibril fragmentation is significicant even when not stirring samples},
