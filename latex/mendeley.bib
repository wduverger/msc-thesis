@article{Chevez-Gutierrez2012,
abstract = {The mechanisms by which mutations in the presenilins (PSEN) or the amyloid precursor protein (APP) genes cause familial Alzheimer disease (FAD) are controversial. FAD mutations increase the release of amyloid $\beta$2 (A$\beta$2)42 relative to A$\beta$240 by an unknown, possibly gain-of-toxic- function, mechanism. However, many PSEN mutations paradoxically impair $\gamma$-secretase and loss-of-functiong mechanisms have also been postulated. Here, we use kinetic studies to demonstrate that FAD mutations affect A$\beta$2 generation via three different mechanisms, resulting in qualitative changes in the A$\beta$2 profiles, which are not limited to A$\beta$242. Loss of $\epsilon$-cleavage function is not generally observed among FAD mutants. On the other hand, $\gamma$-secretase inhibitors used in the clinic appear to block the initial $\epsilon$-cleavage step, but unexpectedly affect more selectively Notch than APP processing, while modulators act as activators of the carboxypeptidase-like ($\gamma$) activity. Overall, we provide a coherent explanation for the effect of different FAD mutations, demonstrating the importance of qualitative rather than quantitative changes in the A$\beta$2 products, and suggest fundamental improvements for current drug development efforts. {\textcopyright} 2012 European Molecular Biology Organization.},
author = {Ch{\'{e}}vez-Guti{\'{e}}rrez, Luc{\'{i}}a and Bammens, Leen and Benilova, Iryna and Vandersteen, Annelies and Benurwar, Manasi and Borgers, Marianne and Lismont, Sam and Zhou, Lujia and {Van Cleynenbreugel}, Simon and Esselmann, Hermann and Wiltfang, Jens and Serneels, Lutgarde and Karran, Eric and Gijsen, Harrie and Schymkowitz, Joost and Rousseau, Frederic and Broersen, Kerensa and {De Strooper}, Bart},
doi = {10.1038/emboj.2012.79},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Ch{\'{e}}vez-Guti{\'{e}}rrez et al. - 2012 - The mechanism of $\gamma$-Secretase dysfunction in familial Alzheimer disease.pdf:pdf},
