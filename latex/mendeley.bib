isbn = {1098-5530 (Electronic)$\backslash$r0021-9193 (Linking)},
issn = {00219193},
journal = {Journal of Bacteriology},
number = {8},
pages = {1793--1798},
pmid = {21296966},
title = {{Indole transport across Escherichia coli membranes}},
volume = {193},
year = {2011}
}
