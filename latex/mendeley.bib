number = {1},
pages = {69--86},
pmid = {20214609},
title = {{Phage Therapy in Clinical Practice: Treatment of Human Infections}},
url = {http://www.eurekaselect.com/openurl/content.php?genre=article{\&}issn=1389-2010{\&}volume=11{\&}issue=1{\&}spage=69},
volume = {11},
