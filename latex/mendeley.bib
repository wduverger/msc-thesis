@article{Tahirbegi2020,
abstract = {Aggregates of amyloid-$\beta$ (A$\beta$) are characteristic of Alzheimer's disease, but there is no consensus as to either the nature of the toxic molecular complex or the mechanism by which toxic aggregates are produced. We report on a novel feature of amyloid-lipid interactions where discontinuities in the lipid continuum can serve as catalytic centers for a previously unseen microscale aggregation phenomenon. We show that specific lipid membrane conditions rapidly produce long contours of lipid-bound peptide, even at sub-physiological concentrations of A$\beta$. Using single molecule fluorescence, time-lapse TIRF microscopy and AFM imaging we characterize this phenomenon and identify some exceptional properties of the aggregation pathway which make it a likely contributor to early oligomer and fibril formation, and thus a potential critical mechanism in the etiology of AD. We infer that these amyloidogenic events occur only at areas of high membrane curvature, which suggests a range of possible mechanisms by which accumulated physiological changes may lead to their inception. The speed of the formation is in hours to days, even at 1 nM peptide concentrations. Lipid features of this type may act like an assembly line for monomeric and small oligomeric subunits of A$\beta$ to increase their aggregation states. We conclude that under lipid environmental conditions, where catalytic centers of the observed type are common, key pathological features of AD may arise on a very short timescale under physiological concentration.},
author = {Tahirbegi, Bogachan and Magness, Alastair J. and Piersimoni, Maria Elena and Kn{\"{o}}pfel, Thomas and Willison, Keith R. and Klug, David R. and Ying, Liming},
doi = {10.1038/s41598-020-66373-3},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Tahirbegi et al. - 2020 - A Novel A$\beta$40 Assembly at Physiological Concentration.pdf:pdf},
isbn = {4159802066373},
issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--10},
pmid = {32528074},
title = {{A Novel A$\beta$40 Assembly at Physiological Concentration}},
volume = {10},
year = {2020}
}
@article{Strohkendl2018,
abstract = {Graphical Abstract Highlights d DNA target binding by CRISPR-Cas12a is rate limiting for DNA cleavage d Multiple cleavage sites are present on each DNA strand d Specificity against mismatches suggests a late transition state for R-loop formation d Data explain in vivo cleavage patterns by Cas12a and differences from Cas9 In Brief Strohkendl et al. dissect DNA binding and cleavage by CRISPR-Cas12a. They show that binding is functionally irreversible, yet Cas12a discriminates against mismatches with the target DNA extending beyond a seed region. These results suggest that R-loop propagation is readily reversible, enabling Cas12a to select DNA sequences more precisely than Cas9. DNA Targeting by CRISPR-Cas12a Reversibility during R-loop propagation ensures high target specificity Upon completion of R-loop, DNA target cleavage is inevitable k PAM k R-loop k c Strohkendl et al., 2018, Molecular Cell 71, 1-9 September 6, 2018 {\textordfeminine} 2018 Elsevier Inc. https://doi. SUMMARY Class 2 CRISPR-Cas nucleases are programmable genome editing tools with promising applications in human health and disease. However, DNA cleavage at off-target sites that resemble the target sequence is a pervasive problem that remains poorly understood mechanistically. Here, we use quantitative kinetics to dissect the reaction steps of DNA targeting by Acidaminococcus sp Cas12a (also known as Cpf1). We show that Cas12a binds DNA tightly in two kinetically separable steps. Pro-tospacer-adjacent motif (PAM) recognition is followed by rate-limiting R-loop propagation, leading to inevitable DNA cleavage of both strands. Despite functionally irreversible binding, Cas12a discriminates strongly against mismatches along most of the DNA target sequence. This result implies substantial reversibility during R-loop formation-a late transition state-and defies common descriptions of a ''seed'' region. Our results provide a quantitative basis for the DNA cleavage patterns measured in vivo and observations of greater reported target specificity for Cas12a than for the Cas9 nuclease.},
author = {Strohkendl, Isabel and Saifuddin, Fatema A and Rybarski, James R and Finkelstein, Ilya J and Correspondence, Rick Russell and Russell, Rick},
doi = {10.1016/j.molcel.2018.06.043},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Strohkendl et al. - 2018 - Kinetic Basis for DNA Target Specificity of CRISPR-Cas12a Molecular Cell Article Kinetic Basis for DNA Target.pdf:pdf},
journal = {Molecular Cell},
title = {{Kinetic Basis for DNA Target Specificity of CRISPR-Cas12a Molecular Cell Article Kinetic Basis for DNA Target Specificity of CRISPR-Cas12a}},
year = {2018}
}
@article{Yang2021,
abstract = {The misfolding and aggregation of polypeptide chains into $\beta$-sheet-rich amyloid fibrils is associated with a wide range of neurodegenerative diseases. Growing evidence indicates that the oligomeric intermediates populated in the early stages of amyloid formation rather than the mature fibrils are responsible for the cytotoxicity and pathology and are potentially therapeutic targets. However, due to the low-populated, transient, and heterogeneous nature of amyloid oligomers, they are hard to characterize by conventional bulk methods. The development of single molecule approaches provides a powerful toolkit for investigating these oligomeric intermediates as well as the complex process of amyloid aggregation at molecular resolution. In this review, we present an overview of recent progress in characterizing the oligomerization of amyloid proteins by single molecule fluorescence techniques, including single-molecule F{\"{o}}rster resonance energy transfer (smFRET), fluorescence correlation spectroscopy (FCS), single-molecule photobleaching and super-resolution optical imaging. We discuss how these techniques have been applied to investigate the different aspects of amyloid oligomers and facilitate understanding of the mechanism of amyloid aggregation.},
annote = {Wu, Chinese Academy of Sciences

Review of luorescence methods to determine amyloid structure
Also mentions environmental factors},
author = {Yang, Jie and Perrett, Sarah and Wu, Si},
doi = {10.3390/molecules26040948},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Yang, Perrett, Wu - 2021 - Single Molecule Characterization of Amyloid Oligomers.pdf:pdf},
journal = {Molecules},
keywords = {amyloid oligomers,neurode-,protein aggregation,single molecule fluorescence detection},
number = {4},
pages = {948},
title = {{Single Molecule Characterization of Amyloid Oligomers}},
volume = {26},
year = {2021}
