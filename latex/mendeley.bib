volume = {11},
year = {2015}
}
@article{Zhang2019a,
abstract = {Polyphosphate (polyP), which is found in various microorganisms and human cells, is an anionic biopolymer consisting of inorganic phosphates linked by high-energy phosphate bonds. Previous studies revealed that polyPs strongly promoted the amyloid formation of several amyloidogenic proteins; however, the mechanism of polyP-induced amyloid formation remains unclear. In the present study using $\beta$2-microglobulin ($\beta$2m), a protein responsible for dialysis-related amyloidosis, we investigated amyloid formation in the presence of various chain lengths of polyPs at different concentrations under both acidic (pH 2.0 to 2.5) and neutral pH (pH 7.0 to 7.5) conditions. We found that the amyloid formation of $\beta$2m at acidic pH was significantly accelerated by the addition of polyPs at an optimal polyP concentration, which decreased with an increase in chain length. The results obtained indicated that electrostatic interactions between positively charged $\beta$2m and negatively charged polyPs play a major role in amyloid formation. Under neutral pH conditions, long polyP with 60 to 70 phosphates induced the amyloid formation of $\beta$2m at several micromoles per liter, a similar concentration range to that in vivo. Since $\beta$2m with an isoelectric point of 6.4 has a slightly negative net charge at pH 7, polyPs were unlikely to interact with $\beta$2m electrostatically. PolyPs appear to dehydrate water molecules around $\beta$2m under the unfolded conformation, leading to the preferential stabilization of less water-exposed amyloid fibrils. These results not only revealed the pH-dependent mechanism of the amyloid formation of $\beta$2m but also suggested that polyPs play an important role in the development of dialysis-related amyloidosis.},
author = {ming Zhang, Chun and Yamaguchi, Keiichi and So, Masatomo and Sasahara, Kenji and Ito, Toru and Yamamoto, Suguru and Narita, Ichiei and Kardos, J{\'{o}}zsef and Naiki, Hironobu and Goto, Yuji},
doi = {10.1073/pnas.1819813116},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Zhang et al. - 2019 - Possible mechanisms of polyphosphate-induced amyloid fibril formation of $\beta$2-microglobulin.pdf:pdf},
issn = {10916490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Amorphous aggregation,Amyloid fibrillation,Amyloidosis,Dialysis-related,Polyphosphates,Supersaturation},
number = {26},
pages = {12833--12838},
pmid = {31182591},
title = {{Possible mechanisms of polyphosphate-induced amyloid fibril formation of $\beta$2-microglobulin}},
volume = {116},
year = {2019}
}
@article{Khurana2005,
abstract = {Thioflavin T is a benzothiazole dye that exhibits enhanced fluorescence upon binding to amyloid fibrils and is commonly used to diagnose amyloid fibrils, both ex vivo and in vitro. In aqueous solutions, thioflavin T was found to exist as micelles at concentrations commonly used to monitor fibrils by fluorescence assay (∼10-20 $\mu$M). Specific conductivity changes were measured at varying concentration of thioflavin T and the critical micellar concentration was calculated to be 4.0 ± 0.5 $\mu$M. Interestingly, changes in the fluorescence excitation and emission of thioflavin T were also dependent on the micelle formation. The thioflavin T micelles of 3 nm diameter were directly visualized using atomic force microscopy, and bound thioflavin T micelles were observed along the fibril length for representative fibrils. Increasing concentration of thioflavin T above the critical micellar concentration shows increased numbers of micelles bound along the length of the amyloid fibrils. Thioflavin T micelles were disrupted at low pH as observed by atomic force microscopy and fluorescence enhancement upon binding of thioflavin T to amyloid fibrils also reduced by several-fold upon decreasing the pH to below 3. This suggests that positive charge on the thioflavin T molecule has a role in its micelle formation that then bind the amyloid fibrils. Our data suggests that the micelles of thioflavin T bind amyloid fibrils leading to enhancement of fluorescence emission. {\textcopyright} 2005 Elsevier Inc. All rights reserved.},
author = {Khurana, Ritu and Coleman, Chris and Ionescu-Zanetti, Cristian and Carter, Sue A. and Krishna, Vinay and Grover, Rajesh K. and Roy, Raja and Singh, Shashi},
doi = {10.1016/j.jsb.2005.06.006},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Khurana et al. - 2005 - Mechanism of thioflavin T binding to amyloid fibrils.pdf:pdf},
issn = {10478477},
journal = {Journal of Structural Biology},
keywords = {Amyloid binding dyes,Atomic force microscopy,Critical micellar concentration,Fluorescence enhancement,Thioflavin T micelles,spotting techniques?},
mendeley-tags = {spotting techniques?},
number = {3},
pages = {229--238},
pmid = {16125973},
title = {{Mechanism of thioflavin T binding to amyloid fibrils}},
volume = {151},
year = {2005}
}
@article{Ruggeri2019,
abstract = {The phenomenon of protein misfolding and aggregation results in the formation of highly heterogeneous protein aggregates, which are associated with neurodegenerative conditions such as Alzheimer's and Parkinson's diseases. In particular low molecular weight aggregates, amyloid oligomers, have been shown to possess generic cytotoxic properties and are implicated as neurotoxins in many forms of dementia. We illustrate the use of methods based on atomic force microscopy (AFM) to address the challenging task of characterizing the morphological, structural and chemical properties of these aggregates, which are difficult to study using conventional structural methods or bulk biophysical methods because of their heterogeneity and transient nature. Scanning probe microscopy approaches are now capable of investigating the morphology of amyloid aggregates with sub-nanometer resolution. We show here that infrared (IR) nanospectroscopy (AFM-IR), which simultaneously exploits the high resolution of AFM and the chemical recognition power of IR spectroscopy, can go further and enable the characterization of the structural properties of individual protein aggregates, and thus offer insights into the aggregation mechanisms. Since the approach that we describe can be applied also to the investigations of the interactions of protein assemblies with small molecules and antibodies, it can deliver fundamental information to develop new therapeutic compounds to diagnose or treat neurodegenerative disorders.},
author = {Ruggeri, Francesco Simone and {\v{S}}neideris, Tomas and Chia, Sean and Vendruscolo, Michele and Knowles, Tuomas P.J.},
doi = {10.3791/60108},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Ruggeri et al. - 2019 - Characterizing individual protein aggregates by infrared nanospectroscopy and atomic force microscopy.pdf:pdf},
issn = {1940087X},
journal = {Journal of Visualized Experiments},
keywords = {Amyloid,Atomic force microscopy,Biochemistry,Infrared nanospectroscopy,Issue 151,Neurodegeneration,Protein aggregation,Single molecule analysis,Spectroscopy,spotting techniques?},
mendeley-tags = {spotting techniques?},
number = {151},
pages = {1--12},
pmid = {31566623},
title = {{Characterizing individual protein aggregates by infrared nanospectroscopy and atomic force microscopy}},
volume = {2019},
year = {2019}
}
@article{Sevcsik2011,
abstract = {Both oxidative stress and aggregation of the protein $\alpha$-synuclein (aS) have been implicated as key factors in the etiology of Parkinson's disease. Specifically, oxidative modifications to aS disrupt its binding to lipid membranes, an interaction considered critical to its native function. Here we seek to provide a mechanistic explanation for this phenomenon by investigating the effects of oxidative nitration of tyrosine residues on the structure of aS and its interaction with lipid membranes. Membrane binding is mediated by the first ∼95 residues of aS. We find that nitration of the single tyrosine (Y39) in this domain disrupts binding due to electrostatic repulsion. Moreover, we observe that nitration of the three tyrosines (Y125/133/136) in the C-terminal domain is equally effective in perturbing binding, an intriguing result given that the C-terminus is not thought to interact directly with the lipid bilayer. Our investigations show that tyrosine nitration results in a change of the conformational states populated by aS in solution, with the most prominent changes occurring in the C-terminal region. These results lead us to suggest that nitration of Y125/133/136 reduces the membrane-binding affinity of aS through allosteric coupling by altering the ensemble of conformational states and depopulating those capable of membrane binding. While allostery is a well-established concept for structured proteins, it has only recently been discussed in the context of disordered proteins. We propose that allosteric regulation through modification of specific residues in, or ligand binding to, the C-terminus may even be a general mechanism for modulating aS function. {\textcopyright} 2011 American Chemical Society.},
author = {Sevcsik, Eva and Trexler, Adam J. and Dunn, Joanna M. and Rhoades, Elizabeth},
doi = {10.1021/ja2009554},
file = {:C$\backslash$:/Users/Wouter Duverger/Documents/Mendeley/Sevcsik et al. - 2011 - Allostery in a disordered protein Oxidative modifications to $\alpha$-synuclein act distally to regulate membrane bindi.pdf:pdf},
issn = {00027863},
journal = {Journal of the American Chemical Society},
number = {18},
pages = {7152--7158},
pmid = {21491910},
title = {{Allostery in a disordered protein: Oxidative modifications to $\alpha$-synuclein act distally to regulate membrane binding}},
volume = {133},
year = {2011}
}
